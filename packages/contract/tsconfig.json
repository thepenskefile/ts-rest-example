{
    "compilerOptions": {
      "target": "es2016", 
      "strict": true,
      "allowJs": true,
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "forceConsistentCasingInFileNames": true,
      "skipLibCheck": true,
      "module": "esnext",
      "moduleResolution":"node",
      "outDir": "./dist",
    },
    "include": ["."],
    "exclude": ["dist", "node_modules", "**/*.spec.ts"]
  }