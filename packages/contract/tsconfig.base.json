{
    "compilerOptions": {
      "strict": true,
      "esModuleInterop": true,
      "noImplicitAny": true,
      "removeComments": true,
      "preserveConstEnums": true,
      "sourceMap": true,
      "allowJs": true,
      "allowSyntheticDefaultImports": true,
      "forceConsistentCasingInFileNames": true,
      "skipLibCheck": true,
      "declaration": true,
      "rootDir": "./src",
      "moduleResolution":"Node",
    }
}